{
  "name": "discord-service",
  "version": "1.0.0",
  "private": true,
  "description": "Dirt Boi bot",
  "scripts": {
    "tsc": "tsc",
    "clean": "rimraf build",
    "assets": "copyfiles \"rpg/**/*.json\" \"build/assets/\"",
    "build": "npm run clean && tsc && npm run assets",
    "recompile": "npm run build && node build/index.js",
    "dev": "nodemon -e js,ts,json --watch src --watch rpg --exec \"npm run recompile\""
  },
