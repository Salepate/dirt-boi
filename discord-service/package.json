    "nodemon": "^1.19.2"
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.2",
    "copyfiles": "^2.1.1",
    "cors": "^2.8.5",
    "discord.js": "^11.5.1",
    "express": "^4.17.1",
    "redux": "^4.0.4",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.3"
  }
}
